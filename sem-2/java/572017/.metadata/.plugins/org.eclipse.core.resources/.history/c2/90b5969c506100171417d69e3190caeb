import java.util.*;
public class Main {
	public static void main(String args[]){
		
		Bitset b1 = new Bitset(16);
		Bitset b2 = new Bitset(16);
		for(int i=0;i<16;i++){
			if(i % 2 == 0){
				b1.set(i);
			}
			if(i % 5 != 0){
				b2.set(i);
			}
				
		}
		System.out.println("b1" + "\n" + b1);
		System.out.println("b2" + "\n" + b2);
		
	}

}
